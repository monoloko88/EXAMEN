package miPaquete;

public class Padre {
    private String name;
    private int age;

    public Padre(String name, int age) {
        this.name = name;
        this.age = age;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public void print() {
        System.out.println("Name: " + name + " Age: " + age);
    }

    public String toString() {
        return "Name: " + name + " Age: " + age;
    }

}
